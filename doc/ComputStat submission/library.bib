isbn = {978-1-4673-2497-7},
keywords = {Bayesian Hierarchical Generative Models of Social,Bayesian methods,Communities,Community Discovery and Role Assignment,Context,Probabilistic Social Network Analysis,Probabilistic logic,Probability distribution,Random variables,Social network services},
