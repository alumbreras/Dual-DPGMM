@article{Krupka2008,
abstract = {We argue that when objects are characterized by many attributes, clustering$\backslash$nthem on the basis of a random subset of these attributes can$\backslash$ncapture information on the unobserved attributes as well. Moreover,$\backslash$nwe show that under mild technical conditions, clustering the objects$\backslash$non the basis of such a random subset performs almost as well as clustering$\backslash$nwith the full attribute set. We prove finite sample generalization$\backslash$ntheorems for this novel learning scheme that extends analogous results$\backslash$nfrom the supervised learning setting. We use our framework to analyze$\backslash$ngeneralization to unobserved features of two well-known clustering$\backslash$nalgorithms: k-means and the maximum likelihood multinomial mixture$\backslash$nmodel. The scheme is demonstrated for collaborative filtering of users$\backslash$nwith movie ratings as attributes and document clustering with words$\backslash$nas attributes.},
